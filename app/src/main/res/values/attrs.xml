<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CommonTitleView">
        <attr name="common_title" format="reference" />
        <attr name="common_back_btn_visible" format="boolean" />
        <attr name="common_settings_btn_visible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Cookie">
        <attr name="cookieTitleColor" format="color"/>
        <attr name="cookieMessageColor" format="color"/>
        <attr name="cookieActionColor" format="color"/>
        <attr name="cookieBackgroundColor" format="color"/>

        <attr name="cookieTitleSize" format="dimension"/>
        <attr name="cookieMessageSize" format="dimension"/>
        <attr name="cookieActionSize" format="dimension"/>
        <attr name="cookiePadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MoreSettingView">
        <attr name="moreSettingName" format="string" />
        <attr name="moreSettingSrc" format="reference" />
    </declare-styleable>

    <declare-styleable name="SettingCellView">
        <attr name="cell_view_type">
            <enum name="text_with_arrow" value="1" />
            <enum name="text_with_switch" value="2" />
        </attr>
        <attr name="cell_view_title" format="string" />
        <attr name="cell_view_content" format="string" />
        <attr name="cell_view_switch_checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SeekBarWithSound">
        <attr name="seekbar_name" format="string" />
    </declare-styleable>
</resources>